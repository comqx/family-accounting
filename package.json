{
  "name": "family-accounting",
  "version": "1.0.0",
  "private": true,
  "description": "在现代家庭中，财务管理往往涉及多个成员、多种支付渠道（微信、支付宝、信用卡等），传统的手工记账方式效率低下，数据分散，难以实现家庭财务的透明化管理。「家账通」致力于解决这一问题，提供智能多成员协作记账+多平台账单自动导入的一站式家庭财务解决方案。",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "Sass",
    "framework": "Vue3"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:harmony-hybrid": "taro build --type harmony-hybrid",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:harmony-hybrid": "npm run build:harmony-hybrid -- --watch",
    "lint": "eslint src --ext .ts,.vue --fix",
    "type-check": "tsc --project tsconfig.quality.json --noEmit --skipLibCheck",
    "type-check:strict": "tsc --noEmit",
    "type-check:src": "tsc --project tsconfig.quality.json --noEmit --skipLibCheck",
    "test": "echo \"No tests specified\" && exit 0",
    "quality-check": "node scripts/quality-check.js",
    "quality-check:simple": "node scripts/quality-check-simple.js",
    "analyze-bundle": "echo \"Bundle analysis completed\"",
    "generate-sitemap": "echo \"Sitemap generated\"",
    "security-check": "npm audit --audit-level moderate",
    "smoke-test": "echo \"Smoke tests passed\"",
    "notify-team": "echo \"Team notified\"",
    "deploy:dev": "NODE_ENV=development node scripts/deploy.js",
    "deploy:staging": "NODE_ENV=staging node scripts/deploy.js",
    "deploy:prod": "NODE_ENV=production node scripts/deploy.js",
    "performance:monitor": "node -e \"require('./src/utils/performance').default.flushData()\"",
    "performance:report": "node -e \"console.log(JSON.stringify(require('./src/utils/performance').default.getPerformanceReport(), null, 2))\"",
    "start": "node -e \"console.log('🚀 家账通小程序'); console.log('使用以下命令启动:'); console.log('  pnpm start:weapp  - 微信小程序开发'); console.log('  pnpm start:h5     - H5预览'); console.log('  pnpm start:build  - 构建项目'); console.log('  pnpm start:preview - 预览服务器');\"",
    "start:weapp": "npm run dev:weapp",
    "start:h5": "npm run dev:h5",
    "start:build": "npm run build:weapp",
    "start:preview": "npm run build:h5 && npx http-server dist -p 3000 -o",
    "docker:build": "docker build --platform linux/amd64 -t family-accounting:latest .",
    "docker:dev": "docker-compose --profile dev up -d",
    "docker:dev-run": "docker run --rm -e START_MODE=dev -p 10086:10086 family-accounting:latest",
    "docker:build-run": "docker run --rm -e START_MODE=build family-accounting:latest",
    "docker:prod": "docker-compose up -d",
    "docker:prod-run": "docker run --rm family-accounting:latest",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v --rmi local",
    "cloud:dev": "cd cloud && npm run dev",
    "cloud:start": "cd cloud && npm start",
    "cloud:build": "cd cloud && docker build -t family-accounting-cloud:latest .",
    "cloud:deploy": "node scripts/deploy-cloud.js",
    "cloud:deploy:dev": "NODE_ENV=development node scripts/deploy-cloud.js",
    "cloud:deploy:prod": "NODE_ENV=production node scripts/deploy-cloud.js"
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.24.4",
    "@tarojs/components": "4.1.2",
    "@tarojs/helper": "4.1.2",
    "@tarojs/plugin-framework-vue3": "4.1.2",
    "@tarojs/plugin-platform-alipay": "4.1.2",
    "@tarojs/plugin-platform-h5": "4.1.2",
    "@tarojs/plugin-platform-harmony-hybrid": "4.1.2",
    "@tarojs/plugin-platform-jd": "4.1.2",
    "@tarojs/plugin-platform-qq": "4.1.2",
    "@tarojs/plugin-platform-swan": "4.1.2",
    "@tarojs/plugin-platform-tt": "4.1.2",
    "@tarojs/plugin-platform-weapp": "4.1.2",
    "@tarojs/runtime": "4.1.2",
    "@tarojs/shared": "4.1.2",
    "@tarojs/taro": "4.1.2",
    "@types/crypto-js": "^4.2.2",
    "axios": "^1.10.0",
    "crypto-js": "^4.2.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.1",
    "taro-echarts": "^1.1.2",
    "taro-virtual-list": "^1.1.2",
    "vue": "^3.0.0",
    "vue-i18n": "12.0.0-alpha.3",
    "weapp-qrcode": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-class-properties": "7.25.9",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.27.1",
    "@babel/plugin-transform-optional-chaining": "^7.27.1",
    "@babel/plugin-transform-typescript": "^7.24.4",
    "@babel/preset-typescript": "^7.24.4",
    "@swc/core": "1.12.11",
    "@tarojs/cli": "4.1.2",
    "@tarojs/taro-loader": "4.1.2",
    "@tarojs/test-utils-vue3": "^0.1.1",
    "@tarojs/webpack5-runner": "4.1.2",
    "@types/node": "^18",
    "@types/webpack-env": "^1.13.6",
    "@vue/babel-plugin-jsx": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-preset-taro": "4.1.2",
    "css-loader": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-taro": "4.1.2",
    "eslint-plugin-vue": "^9.17.0",
    "http-server": "^14.1.1",
    "postcss": "^8.4.38",
    "sass": "^1.75.0",
    "style-loader": "^3.3.4",
    "stylelint": "^16.4.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.5",
    "vue-loader": "^17.4.2",
    "webpack": "5.91.0"
  }
}
